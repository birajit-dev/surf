            "publisher":{
                "@type":"Northeast Surf",
                "name":"<%= teerdata.author %>",
                "logo":"https://northeastsurf.com/logo.png"
            },
            "headline": "<%= teerdata.teer_name %>",
            "mainEntityOfPage": "https://northeastsurf.com/teeresult/<%= teerdata.teer_category %>/<%= teerdata.teer_permalink %>",
            "articleBody": "<%= teerdata.meta_description %>",
            "image":"<%= teerdata.post_image %>",
            "datePublished":"<%= teerdata.update_date %>"
            }
            </script>
    
    
            <!-- Bootstrap, Font Awesome, Aminate, Owl Carausel, Normalize CSS -->
            <link href="/css/css-bootstrap.css" rel="stylesheet">
