            <script src="/js/js-jquery.bpopup.min.js"></script>
            <script>
            $( document ).ready(function() {
                $('#popup_this').bPopup();
            });
            </script>
    
            <link href='https://fonts.googleapis.com/css?family=Droid Serif' rel='stylesheet'>
            <%- include('alltags.ejs'); %> 
    
        </head>
        <body class="single">
            <div class="top-scroll-bar"></div>
           <%- include('header.ejs'); %>
                <main id="content">
                    <div class="container">
                        <div class="entry-header">                        
                            <div class="mb-5">
                                <h1 class="entry-title m_b_2rem" style="font-family: Droid Serif;">
    <%= teerdata.teer_name %></h1>
    
                                <div class="entry-meta align-items-center">
                                    <a class="author-avatar" href="#">
                                        
                                        <img src="https://media.northeastsurf.com/author-surf.png" alt=""></a>
                                    <a href="#">Northeast Surf</a> in <a href="/">Surf&trade;</a><br>                                    
                                    <span><%= teerdata.teer_date %></span>
                                    <span class="middotDivider"></span>
                                    <span class="readingTime" title="3 min read">3 min read</span>
                                    <span class="svgIcon svgIcon--star">
                                        <svg class="svgIcon-use" width="15" height="15">
                                            <path d="M7.438 2.324c.034-.099.09-.099.123 0l1.2 3.53a.29.29 0 0 0 .26.19h3.884c.11 0 .127.049.038.111L9.8 8.327a.271.271 0 0 0-.099.291l1.2 3.53c.034.1-.011.131-.098.069l-3.142-2.18a.303.303 0 0 0-.32 0l-3.145 2.182c-.087.06-.132.03-.099-.068l1.2-3.53a.271.271 0 0 0-.098-.292L2.056 6.146c-.087-.06-.071-.112.038-.112h3.884a.29.29 0 0 0 .26-.19l1.2-3.52z"></path>
                                        </svg>
                                    </span>
    
                                    
                                    
                                </div>
                                
    
                                
                            </div>
    <!-- ShareThis BEGIN --><div class="sharethis-inline-share-buttons" style="margin-top: -35px; padding-bottom:10px"></div><!-- ShareThis END -->
    
                            <figure class="image zoom mb-5">
                                <% if(teerdata.teer_category=="guwahati"){
                                    %>
                                    <img src="https://media.northeastsurf.com/khanapara_teer.jpeg" alt="<%= teerdata.teer_name %>" style="justify-content: center;width: 100%;" />
                                    <%
                                    }
                                    else{
                                    %>
                                    <img src="https://media.northeastsurf.com/shillong_teer.webp" alt="<%= teerdata.teer_name %>" style="justify-content: center;width: 100%;" />
                                    <%
                                    }
                                    %>
                                
                                
                            </figure>  <!--figure-->
                        </div> <!--end single header-->
                        
                        <article class="entry-wraper mb-5">
                            
                           
                            <div class="entry-main-content" style="font-family: Droid Serif;">
    
                                    <%- teerdata.teer_content; %>
    
    
                                
    
                                
                            </div>
                            <div class="entry-bottom">
                                <div class="tags-wrap heading">
                                    <span class="tags">
                                        <% //Loading the variable
                                        var mystr = teerdata.teer_tags;
                                        
                                        //Splitting it with : as the separator
                                        var myarr = mystr.split(",");
                                        
                                        //Resulting array structure
                                        //myarr = ['0000000020C90037', 'TEMP', 'data'];
                                        for(var i = 0; i < myarr.length; i++) { %>
                                            <a href="#" rel="tag"><%= myarr[i] %></a>
    
                                          <% } %>
                                    </span>
                                </div>
                            </div>
                            
                        </article> <!--entry-content-->
                        
                        <!--Begin post related-->
                        <div class="related-posts mb-5">
                            <h4 class="spanborder text-center">
                                <span>Previeous Result</span>
                            </h4>
                            <div class="row justify-content-between">
                                 <div class="divider-2"></div>   
                                 
                                 <% if( teerRelated != ''){
                                    teerRelated.forEach(function(dnews, index){ %>
                                <article class="col-md-4">
                                    <div class="mb-3 d-flex row">
                                        <figure class="col-md-5"><a href="/teer/result/2022/<%- dnews.teer_permalink %>">
                                            <% if(dnews.teer_category=="guwahati"){
                                                %>
                                                <img src="https://media.northeastsurf.com/khanapara_teer.jpeg" alt="<%= dnews.teer_name %>"/>
                                                <%
                                                }
                                                else{
                                                %>
                                                <img src="https://media.northeastsurf.com/shillong_teer.webp" alt="<%= dnews.teer_name %>" />
                                                <%
                                                }
                                                %>                                        
                                        </a></figure>
                                        <div class="entry-content col-md-7 pl-md-0">                                    
                                            <h5 class="entry-title mb-3"><a href="/teer/result/2022/<%- dnews.teer_permalink %>"><%- dnews.teer_name %></a></h5>
                                            <div class="entry-meta align-items-center">
                                                <a href="#">NE Surf</a> in <a href="#">Surf&trade;</a><br>                                    
                                                <span><%- dnews.teer_date %></span>
                                                <span class="middotDivider"></span>
                                                <span class="readingTime" title="3 min read">6 min</span>
                                                <span class="svgIcon svgIcon--star">
                                                    <svg class="svgIcon-use" width="15" height="15">
                                                        <path d="M7.438 2.324c.034-.099.09-.099.123 0l1.2 3.53a.29.29 0 0 0 .26.19h3.884c.11 0 .127.049.038.111L9.8 8.327a.271.271 0 0 0-.099.291l1.2 3.53c.034.1-.011.131-.098.069l-3.142-2.18a.303.303 0 0 0-.32 0l-3.145 2.182c-.087.06-.132.03-.099-.068l1.2-3.53a.271.271 0 0 0-.098-.292L2.056 6.146c-.087-.06-.071-.112.038-.112h3.884a.29.29 0 0 0 .26-.19l1.2-3.52z"></path>
                                                    </svg>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </article>
                                <%
                        })
                    } %>
                          
                                
                                
                            </div>
                        </div>
                        <!--End post related-->
    
    
                    </div> <!--container-->
                </main>
                <%- include('footer.ejs'); %> 
    
            </div> <!--#wrapper-->
    
            <a href="#" class="back-to-top heading"><i class="icon-left-open-big"></i><span  class="d-lg-inline d-md-none">Top</span></a>
            
            <!--Scripts-->
            <script src="/js/js-bootstrap.js"></script>
            <script src="/js/js-jquery-scrolltofixed-min.js"></script>
            <script src="/js/js-theia-sticky-sidebar.js"></script>
            <script src="/js/js-scripts.js"></script>
        </body>
    </html>
    
    <% } else { %>
        <script>location.replace("/en/in/404")
        </script>
    <% } %>